    "# Crie um array 2D de tamanho (5, 5) com valores aleatórios entre 0 e 100\n",
    "\n",
    "import numpy as np\n",
    "\n",
    "array_2D = np.random.randint(100, size=(5, 5))\n",
    "\n",
    "print (array_2D)\n",
    "\n",
    "# Calcule a média de cada linha\n",
    "cont = 1\n",
    "for i in array_2D:\n",
    "    print (f'Média da Linha {cont} -> {i.mean()}')\n",
    "    cont += 1\n",
    "\n",
    "# Encontre o valor máximo e mínimo de toda a matriz\n",
    "\n",
    "print (f'Valor Máximo -> {array_2D.max()}')\n",
    "print (f'Valor Mínimo -> {array_2D.min()}')\n",
    "\n"
